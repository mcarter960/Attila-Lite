{
	"name": "attila-lite",
	"description": "Attila-Lite Ghost Theme",
	"demo": "https://attila-lite.devops10.com",
	"version": "2.0.0",
	"engines": {
		"ghost": ">=3.0.0",
		"ghost-api": "v3"
	},
	"license": "MIT",
	"screenshots": {
		"desktop": "src/screenshot-desktop.jpg",
		"mobile": "src/screenshot-mobile.jpg"
	},
	"author": {
		"name": "Arthur Carter",
		"email": "mark@carterstein.com",
		"url": "https://arthur.carterstein.com"
	},
	"gpm": {
		"type": "theme",
		"categories": [ "Minimal", "Personal Blogs" ]
	},
	"keywords": [ "ghost-theme", "ghost", "theme", "attila-lite" ],
	"repository": {
		"type": "git",
		"url": "https://github.com/mcarter960/Attila-Lite.git"
	},
	"dependencies": {
		"ghosthunter": "github:jamalneufeld/ghostHunter"
	},
	"devDependencies": {
		"autoprefixer": "^9.1.5",
		"grunt": "^1.5.2",
		"grunt-contrib-clean": "^1.1.0",
		"grunt-contrib-compress": "^1.6.0",
		"grunt-contrib-copy": "^1.0.0",
		"grunt-contrib-uglify": "^3.3.0",
		"grunt-contrib-watch": "^1.0.0",
		"grunt-postcss": "^0.9.0",
		"grunt-sass": "^2.1.0",
		"load-grunt-tasks": "^3.5.2"
	},
	"config": {
		"posts_per_page": 9,
		"image_sizes": {
			"s": {
				"width": 320
			},
			"m": {
				"width": 640
			},
			"l": {
				"width": 960
			},
			"xl": {
				"width": 1920
			}
		}
	},
	"browserslist": [ "defaults" ]
}
